<idea-plugin>
  <version>1.0-SNAPSHOT</version>
  <change-notes>
      Initial release.&lt;br&gt;
      </change-notes>
  <idea-version since-build="191.6183" until-build="191.*"/>
  <id>com.innometrics.innometrics-intellij-plugin.innometrics-intellij-plugin</id>
  <name>Innometrics</name>
  <vendor email="b.khabirov@innopolis.ru" url="htpps://t.me/khabiroff">Bulat Khabirov, Innopolis University</vendor>
  <description>
    Innometrics tracker plugin for IntelliJ platform IDEs&lt;br&gt;
    </description>
  <depends>com.intellij.modules.lang</depends>
  <extensions defaultExtensionNs="com.intellij"/>
  <actions>
    <action id="Innometrics.ToolsMenuItem" class="ToolsMenuAction" text="Innometrics Plugin" description="Innometrics plugin tools menu item">
      <add-to-group group-id="ToolsMenu" anchor="last"/>
    </action>
  </actions>
</idea-plugin>
